{
  "name": "AutoFindXss",
  "version": "2.0",
  "description": "Automated detection of XSS vulnerabilities",
  "manifest_version": 2,
  "content_scripts": [{
    "matches": [
      "http://*",
      "https://*"
    ],
    "js": [
      "./content.js"
    ],
    "run_at": "document_end"
  }],
  "background": {
    "scripts": ["./background.js"],
    "persistent": true
  },
  "permissions": [
    "tabs ",
    "webRequest",
    "webRequestBlocking",
    "http://*",
    "https://*"
  ]
}
